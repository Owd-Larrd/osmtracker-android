<?xml version="1.0" encoding="utf-8"?>
<me.guillaumin.android.osmtracker.layout.DisablableTableLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:id="@+id/tracklogger_tblAmenity">
	<TableRow android:layout_width="fill_parent" 
		android:layout_height="fill_parent" android:layout_weight="1">
		<Button android:id="@+id/tracklogger_amenity_btnBench" android:drawableTop="@drawable/button_amenity_bench"
			android:layout_width="fill_parent" android:layout_weight="1"
			android:layout_height="fill_parent" android:text="@string/tracklogger_amenity_btnBench"></Button>
		<Button android:id="@+id/tracklogger_amenity_btnWater"
			android:drawableTop="@drawable/button_amenity_water"
			android:layout_weight="1" android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:text="@string/tracklogger_amenity_btnWater"></Button>
		<Button android:id="@+id/tracklogger_amenity_btnPharmacy"
			android:drawableTop="@drawable/button_amenity_pharmacy"
			android:layout_width="fill_parent" android:layout_height="fill_parent"
			android:layout_weight="1" android:text="@string/tracklogger_amenity_btnPharmacy"></Button>
	</TableRow>
	<TableRow android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:layout_weight="1">
		<Button android:id="@+id/tracklogger_amenity_btnShop"
			android:drawableTop="@drawable/button_amenity_shop"
			android:layout_width="fill_parent" android:layout_weight="1"
			android:layout_height="fill_parent" android:text="@string/tracklogger_amenity_btnShop"></Button>
		<Button android:id="@+id/tracklogger_amenity_btnMarina"
			android:drawableTop="@drawable/button_amenity_marina"
			android:layout_weight="1" android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:text="@string/tracklogger_amenity_btnMarina"></Button>
		<Button android:id="@+id/tracklogger_amenity_btnSport"
			android:drawableTop="@drawable/button_amenity_sport"
			android:layout_width="fill_parent" android:layout_height="fill_parent"
			android:layout_weight="1" android:text="@string/tracklogger_amenity_btnSport"></Button>
	</TableRow>
	<TableRow android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:layout_weight="1">
		<Button android:id="@+id/tracklogger_amenity_btnTaxi"
			android:drawableTop="@drawable/button_amenity_taxi"
			android:layout_width="fill_parent" android:layout_weight="1"
			android:layout_height="fill_parent" android:text="@string/tracklogger_amenity_btnTaxi"></Button>
		<Button android:id="@+id/tracklogger_amenity_btnDoctors"
			android:drawableTop="@drawable/button_amenity_doctors"
			android:layout_weight="1" android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:text="@string/tracklogger_amenity_btnDoctors"></Button>
		<Button android:id="@+id/tracklogger_amenity_btnRecycling"
			android:drawableTop="@drawable/button_amenity_recycling"
			android:layout_width="fill_parent" android:layout_height="fill_parent"
			android:layout_weight="1" android:text="@string/tracklogger_amenity_btnRecycling"></Button>
	</TableRow>

	<TableRow android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:layout_weight="1">
		<Button android:id="@+id/tracklogger_amenity_btnWorship"
			android:drawableTop="@drawable/button_amenity_place_of_worship"
			android:layout_width="fill_parent" android:layout_weight="1"
			android:layout_height="fill_parent" android:text="@string/tracklogger_amenity_btnWorship"></Button>
		<Button android:id="@+id/tracklogger_amenity_btnPostOffice"
			android:drawableTop="@drawable/button_amenity_post_office"
			android:layout_weight="1" android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:text="@string/tracklogger_amenity_btnPostOffice"></Button>
		<Button android:id="@+id/tracklogger_amenity_btnLibrary"
			android:drawableTop="@drawable/button_amenity_library"
			android:layout_width="fill_parent" android:layout_height="fill_parent"
			android:layout_weight="1" android:text="@string/tracklogger_amenity_btnLibrary"></Button>
	</TableRow>

</me.guillaumin.android.osmtracker.layout.DisablableTableLayout>
