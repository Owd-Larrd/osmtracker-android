<?xml version="1.0" encoding="utf-8"?>
<me.guillaumin.android.osmtracker.layout.DisablableTableLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:id="@+id/tracklogger_tblMisc">
	<TableRow android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:layout_weight="1">
		<Button android:id="@+id/tracklogger_misc_btnBusStop" android:drawableTop="@drawable/button_misc_bus_stop"
			android:layout_width="fill_parent" android:layout_weight="1"
			android:layout_height="fill_parent" android:text="@string/tracklogger_misc_btnBusStop"></Button>
		<Button android:id="@+id/tracklogger_misc_btnRailway" android:drawableTop="@drawable/button_misc_railway"
			android:layout_weight="1" android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:text="@string/tracklogger_misc_btnRailway"></Button>
		<Button android:id="@+id/tracklogger_misc_btnTelephone" android:drawableTop="@drawable/button_misc_telephone"
			android:layout_width="fill_parent" android:layout_height="fill_parent"
			android:layout_weight="1" android:text="@string/tracklogger_misc_btnTelephone"></Button>
	</TableRow>
	<TableRow android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:layout_weight="1">
		<Button android:id="@+id/tracklogger_misc_btnPostBox" android:drawableTop="@drawable/button_misc_post_box"
			android:layout_width="fill_parent" android:layout_weight="1"
			android:layout_height="fill_parent" android:text="@string/tracklogger_misc_btnPostBox"></Button>
		<Button android:id="@+id/tracklogger_misc_btnATM" android:drawableTop="@drawable/button_misc_atm"
			android:layout_weight="1" android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:text="@string/tracklogger_misc_btnATM"></Button>
		<Button android:id="@+id/tracklogger_misc_btnBollard" android:drawableTop="@drawable/button_misc_bollard"
			android:layout_width="fill_parent" android:layout_height="fill_parent"
			android:layout_weight="1" android:text="@string/tracklogger_misc_btnBollard"></Button>
	</TableRow>
	<TableRow android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:layout_weight="1">
		<Button android:id="@+id/tracklogger_misc_btnToilets" android:drawableTop="@drawable/button_misc_toilets"
			android:layout_width="fill_parent" android:layout_weight="1"
			android:layout_height="fill_parent" android:text="@string/tracklogger_misc_btnToilets"></Button>
		<Button android:id="@+id/tracklogger_misc_btnShelter" android:drawableTop="@drawable/button_misc_shelter"
			android:layout_weight="1" android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:text="@string/tracklogger_misc_btnShelter"></Button>
		<Button android:id="@+id/tracklogger_misc_btnSurveillance" android:drawableTop="@drawable/button_misc_surveillance"
			android:layout_width="fill_parent" android:layout_height="fill_parent"
			android:layout_weight="1" android:text="@string/tracklogger_misc_btnSurveillance"></Button>
	</TableRow>


</me.guillaumin.android.osmtracker.layout.DisablableTableLayout>
